{
  "name": "cifio",
  "version": "1.0.0",
  "devDependencies": {
    "json-graph-specification" : "jsongraph/json-graph-specification",
    "mio" : "mandreyel/mio",
    "rapidjson" : "Tencent/rapidjson",
    "zlib" : "madler/zlib",
    "openssl": "openssl/openssl#OpenSSL_1_1_1g",
    "doctest" : "onqtam/doctest"
  }, 
  "description": "input/output of cif data",
  "main": "index.js",
  "scripts": {
    "clean": "make clean",
    "zlib" : "cd node_modules/zlib && cmake -DCMAKE_INSTALL_PREFIX=dist . && make install",
    "ssl": "cd node_modules/openssl && ./config shared --prefix=`pwd`/openssl --openssldir=`pwd`/openssl/openssl && make && make install",
    "build": "cd test && pwd && mkdir -p build/src/parsing && mkdir -p build/src/constitution && which make && make -f Makefile all",
    "test": "yarn build && cd test && pwd && ./test --test-case='test 3sgs' "
  },
  "author": "rimmartin",
  "license": "MIT"
}

